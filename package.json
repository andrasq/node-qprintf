{
  "name": "qprintf",
  "version": "0.13.1",
  "main": "qprintf.js",
  "description": "fast standard C compatible printf",
  "author": "andras@andrasq.com",
  "license": "Apache-2.0",
  "repository": {
    "url": "git://github.com/andrasq/node-qprintf",
    "type": "git"
  },
  "engines": {
    "node": ">=0.10"
  },
  "keywords": [
    "fast",
    "sprintf",
    "Andras",
    "printf"
  ],
  "scripts": {
    "test": "qnit --version && qnit test* || echo qnit not installed",
    "coverage": "nyc --version && nyc --reporter text --reporter lcov qnit test* || echo nyc not installed",
    "clean": "rm -rf .nyc_output coverage out",
    "distclean": "rm -rf .nyc_output coverage out t.c a.out"
  }
}
